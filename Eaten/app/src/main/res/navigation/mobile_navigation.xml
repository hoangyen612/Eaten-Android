<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation.xml"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.eaten.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <activity
        android:id="@+id/searchActivity"
        android:name="com.example.eaten.SearchActivity"
        android:label="activity_search"
        tools:layout="@layout/activity_search" />

    <activity
        android:id="@+id/accInfoActivity"
        android:name="com.example.eaten.AccInfoActivity"
        android:label="activity_acc_info"
        tools:layout="@layout/activity_acc_info" />
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.eaten.MainActivity"
        android:label="MainActivity"
        tools:layout="@layout/activity_main"/>
    <activity
        android:id="@+id/registerActivity"
        android:name="com.example.eaten.RegisterActivity"
        android:label="RegisterActivity"
        tools:layout="@layout/activity_register" />

    <activity
        android:id="@+id/postActivity"
        android:name="com.example.eaten.PostActivity"
        android:label="PostActivity"
        tools:layout="@layout/activity_post"/>
    <activity
        android:id="@+id/homeActivity"
        android:name="com.example.eaten.HomeActivity"
        android:label="activity_home"
        tools:layout="@layout/activity_home" />
    <activity
        android:id="@+id/notificationsActivity"
        android:name="com.example.eaten.NotificationsActivity"
        android:label="NotificationsActivity"
        tools:layout="@layout/activity_notifications" />
</navigation>